[DEFAULT]
container_images_file = /home/stack/containers-prepare-parameter.yaml
local_interface = {{ undercloud_local_interface }}
local_ip = 192.168.24.1/24
undercloud_admin_host = 192.168.24.3
undercloud_ntp_servers = clock.redhat.com,clock2.redhat.com
undercloud_public_host = 192.168.24.2
undercloud_timezone = UTC
undercloud_enable_paunch = false
{% if clean_nodes %}
clean_nodes = True
{% endif %}

[ctlplane-subnet]
cidr = 192.168.24.0/24
dhcp_end = 192.168.24.150
dhcp_start = 192.168.24.100
gateway = 192.168.24.1
inspection_iprange = 192.168.24.10,192.168.24.50
masquerade = true
